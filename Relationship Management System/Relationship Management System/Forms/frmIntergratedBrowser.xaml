<UserControl x:Class="Relationship_Management_System.Forms.frmIntergratedBrowser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Relationship_Management_System.Forms"
        mc:Ignorable="d"
        d:DesignHeight="500" d:DesignWidth="650" 
        Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded"
        >
    <Grid>
        <WebBrowser x:Name="wbrMain" Margin="0,45,0,0" Navigated="wbrMain_Navigated" />
        <TextBox x:Name="txtAddressBar" Height="23" Margin="70,2,215,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Right" Margin="0,4,93,0" VerticalAlignment="Top" Width="75" Click="btnBack_Click"/>
        <Button x:Name="btnForward" Content="Forward" Margin="0,4,9,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="btnForward_Click"/>
        <Label Content="Address:" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
