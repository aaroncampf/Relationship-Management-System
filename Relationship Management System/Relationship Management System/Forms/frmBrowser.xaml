<UserControl x:Class="Relationship_Management_System.Forms.frmBrowser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:cefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Loaded="UserControl_Loaded">
    <Grid>
        <cefSharp:ChromiumWebBrowser x:Name="wbrMain" Grid.Row="0" Address="https://github.com/cefsharp/CefSharp/wiki/Frequently-asked-questions" FrameLoadEnd="wbrMain_FrameLoadEnd" Margin="0,28,0,0" FrameLoadStart="wbrMain_FrameLoadStart" />
        <TextBox x:Name="txtAddress" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="243"/>
        <Button x:Name="btnGo" Content="Go" HorizontalAlignment="Left" Margin="248,1,0,0" VerticalAlignment="Top" Width="52" Click="txtGo_Click"/>
    </Grid>
</UserControl>
